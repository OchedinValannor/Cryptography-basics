                onAction="#handleCancelButton" prefHeight="25.0" prefWidth="150.0" text="Cancel"
                AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="280.0"/>
        <Label alignment="CENTER" layoutX="20.0" layoutY="213.0" prefHeight="25.0" prefWidth="150.0" text="Data storage"
               AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="385.0">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
        <Button fx:id="saveToFileButton" layoutX="5.0" layoutY="313.0" mnemonicParsing="false"
                onAction="#handleSaveToFileButton" prefHeight="25.0" prefWidth="150.0" text="Save to file"
                AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="430.0"/>
        <Button fx:id="loadFileButton" layoutX="15.0" layoutY="323.0" mnemonicParsing="false"
                onAction="#handleLoadFileButton" prefHeight="25.0" prefWidth="150.0" text="Load file"
                AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="475.0"/>
        <Label alignment="CENTER" layoutX="150.0" layoutY="558.0" prefWidth="300.0"

            <font>
                <Font size="13.0"/>
            </font>
        </Label>
        <TextField fx:id="keyField" layoutX="21.0" layoutY="62.0" AnchorPane.leftAnchor="20.0"
                   AnchorPane.topAnchor="55.0"/>
        <Label alignment="CENTER" prefHeight="25.0" prefWidth="150.0" text="Encryption" AnchorPane.leftAnchor="20.0"
               AnchorPane.topAnchor="145.0">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
        <Button fx:id="encryptButton" mnemonicParsing="false" onAction="#handleEncryptButton" prefHeight="25.0"
                prefWidth="150.0" text="Encrypt" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="190.0"/>
        <Button fx:id="decryptButton" layoutX="20.0" layoutY="69.0" mnemonicParsing="false"
                onAction="#handleDecryptButton" prefHeight="25.0" prefWidth="150.0" text="Decrypt"
